{% block transaction_list %}
    <sw-page class="transaction-list">
        <template slot="content">
            {% block transaction_list_content %}
            <transactions-list
                v-if="transactions"
                :items="transactions"
                :repository="repository"
                :showSelection="false"
                :columns="columns">
            </transactions-list>
            {% endblock %}
        </template>
    </sw-page>
{% endblock %}
